@Styles.Render("~/Styles/Books/Fields.css")
<div class="books-content">
    @for (int i = 0; i < Model.Count; i++)
    {
        <a class ="field-panel" href="@Url.Action("FieldBooksExtension", "Books", new { id = Convert.ToInt16(Model[i].F_Id) })">
                <div class="panel-image">
                </div>
                <div class="panel-description">
                    <p class="panel-description-text">@Model[i].F_Name</p>
                </div>
        </a>
     }

</div>